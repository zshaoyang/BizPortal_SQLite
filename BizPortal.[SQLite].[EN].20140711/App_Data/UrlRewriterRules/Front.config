    <UrlFormat>/search-products-{0}-{1}-p{2}/{3}.aspx</UrlFormat>
    <LookFor><![CDATA[^/search\-products\-(?<CategoryId>\d+)\-(?<Level2CategoryId>\d+)\-p(?<page>\d+)/(?<kw>.+?)\.aspx$]]></LookFor>
    <SendTo>SearchProduct.aspx</SendTo>
    <Parameters>
      <Item>CategoryId</Item>
      <Item>Level2CategoryId</Item>
      <Item>page</Item>
      <Item>kw</Item>
