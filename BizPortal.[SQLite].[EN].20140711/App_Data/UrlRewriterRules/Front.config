<?xml version="1.0" encoding="utf-8" ?>
<UrlRewriter>
  <Rule>
    <Name>HomePage</Name>
    <UrlFormat>/</UrlFormat>
    <LookFor>^/$</LookFor>
    <SendTo>Index.aspx</SendTo>
  </Rule>
  <Rule>
    <Name>DefaultPage</Name>
    <UrlFormat>/Default.aspx</UrlFormat>
    <LookFor>^/Default\.aspx</LookFor>
    <SendTo>Index.aspx</SendTo>
  </Rule>
  <Rule>
    <Name>ProductListPage</Name>
    <UrlFormat>/product-list.aspx</UrlFormat>
    <LookFor><![CDATA[^/product\-list\.aspx$]]></LookFor>
    <SendTo>ProductList.aspx</SendTo>
  </Rule>
  <Rule>
    <Name>XProductListPage</Name>
    <UrlFormat>/product-list-{0}.aspx</UrlFormat>
    <LookFor><![CDATA[^/product\-list\-(?<page>\d+)\.aspx$]]></LookFor>
    <SendTo>ProductList.aspx</SendTo>
    <Parameters>
      <Item>page</Item>
    </Parameters>
  </Rule>
  <Rule>
